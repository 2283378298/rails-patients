= simple_form_for (@patient) do |f|
  .form-inputs
    = f.input :first_name
    = f.input :middle_name
    = f.input :last_name
    = f.input :birth,as: :date
    = f.input :gender
    = f.input :status,collection: Patient::STATUS.map{|x| [x[1], x[0]]}, include_blank: false
    = f.button :submit, :class=>"btn btn-default"

